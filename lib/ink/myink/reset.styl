resetUl()
    list-style none

resetButton(background_color=transparent, border=none)
    background-color background_color
    border border


address
    font-style normal
a
    text-decoration none

figure, dl, body, ul
    margin 0
ul
    padding 0
dd, ul
    margin-left 0

table
    border-collapse collapse
th
    text-align left
h1,h2,h3,h4,h5,h6,p
    &:last-child
        margin-bottom 0
    &:first-child
        margin-top 0
